    %div{:id=>"more_frecuency_#{task_program.id}", :class=>'hide'}
      -delayed_jobs[1..delayed_jobs.count].each do |dj|            
        =render :partial=>'delayed_job', :locals=>{:delayed_job=> dj}   
    %sub.more{:name=>"more_frecuency_#{task_program.id}"}
